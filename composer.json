{
    "name": "halo123450/laravel-model-cleanup",
    "description": "This package deletes unneeded records in a database.",
    "keywords": [
        "halo123450",
        "laravel-model-cleanup"
    ],
    "homepage": "https://github.com/halo123450/laravel-model-cleanup",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "tan840027@qq.com",
            "homepage": "https://github.com/halo123450",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^11.0",
        "illuminate/support": ">=11"
    },
    "autoload": {
        "psr-4": {
            "Halo123450\\ModelCleanup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Halo123450\\ModelCleanup\\ModelCleanupServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
